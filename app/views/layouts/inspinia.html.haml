!!!
%html
  %head
    = display_meta_tags
    = yield :meta_tags
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = stylesheet_link_tag    'https://fonts.googleapis.com/css?family=Open+Sans&subset=latin,cyrillic'
    = javascript_include_tag 'application', 'data-turbolinks-track' => true, debug: false
    = javascript_include_tag 'inspinia'
    = yield :javascript_includes
    = csrf_meta_tags
  %body.top-navigation
    = yield :immediate_after_body
    #underlay{style:'height: 50px;'}
    #wrapper
      #page-wrapper.gray-bg
        .row.border-bottom.white-bg
          = render partial:'inspinia/navigation'
        #wrapper.wrapper-content
          = yield
          = render partial:'inspinia/footer'
